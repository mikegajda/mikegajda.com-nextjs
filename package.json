    "embla-carousel-autoplay": "^6.2.0",
    "embla-carousel-react": "^6.2.0",
    "gray-matter": "^4.0.2",
    "groq": "^2.29.3",
    "jimp": "^0.16.1",
    "mdx-prism": "^0.3.3",
    "next": "^11.0.1",
    "next-mdx-remote": "^3.0.2",
    "next-themes": "^0.0.15",
    "potrace": "^2.1.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
